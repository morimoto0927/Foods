<h1>検索結果</h1>
<table>
  <thead>
    <tr>
      <th>住所</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @results.each do |post| %>
      <tr>
        <td><%= post.address %></td>
        <td><%= link_to 'Show', post %></td>
      </tr>
    <% end %>
  </tbody>
</table>

