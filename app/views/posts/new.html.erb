<%= form_with model: @post, local: true do |f| %>

<p><%= f.label :title %><br>
<%= f.text_field :title %>
</p>

<p><%= f.label :address %><br>
<%= f.text_field :address %>
</p>

<p><%= f.label :body %><br>
<%= f.text_area :body %>
</p>

<p><%= f.label :tag_list %><br>
<%= f.text_field :tag_list, value: @post.tag_list.join(',') %>
</p>

<p><%= f.label :image %><br>
<%= f.attachment_field :image %>
</p>
<p><%= f.submit %></p>
<% end %>
